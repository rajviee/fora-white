{
  "expo": {
    "name": "foratask",
    "slug": "foratask",
    "version": "1.0.0",
    "scheme": "foratask",
    "web": {
      "favicon": "./assets/web_logo.png",
      "bundler": "metro"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-secure-store"
    ],
    "orientation": "portrait",
    "icon": "./assets/app_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/app_logo.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/app_logo.png",
      "bundleIdentifier": "com.devtushar.foratask",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.devtushar.foratask",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "73ec8829-b5ed-4ef9-b06c-1a440b5a60de"
      }
    }
  }
}