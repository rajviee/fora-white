{
  "name": "foratask",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "build": "npx expo export",
    "check": "npx expo-doctor",
    "dev:a": "npm run check && eas build --platform android --profile development",
    "dev:i": "eas build --platform ios --profile development",
    "prod:a": "eas build --platform android --profile production",
    "prod:i": "eas build --platform ios --profile production"
  },
